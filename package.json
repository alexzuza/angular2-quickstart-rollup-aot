  "scripts": {
    "start": "rimraf src-es5 && tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "prod-build": "npm run clean-prod && ngc -p tsconfig.aot.json && tsc -p build/tsconfig.prod.json && gulp bundle-src && npm run prod-serve",
    "prod-serve": "lite-server -c prod/bs-config.js",
    "clean-prod": "rimraf src-aot temp/app-prod-compiled prod/app.min.js",
    "clean-dev": "rimraf src-es5 temp",
    "postinstall": "npm run clean-dev && tsc -p build/tsconfig.rxjs-to-es6.json && gulp bundle-rxjs",
    "clean": "npm run clean-dev && npm run clean-prod"
  },
